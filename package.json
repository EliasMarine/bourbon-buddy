{
  "name": "bourbon-buddy",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "// Development scripts": "",
    "// Use dev for regular development without real-time features": "",
    "dev": "next dev",
    "dev:socket": "node server.js",
    "// Use dev:realtime for WebSockets, chat, and streaming features": "",
    "dev:realtime": "ts-node -P scripts/tsconfig.json scripts/pre-startup.ts && node server.js",
    "// Build and deployment scripts": "",
    "build": "node scripts/fix-database-connection.js && node scripts/fix-next-auth-imports.js && next build",
    "start": "next start",
    "start:socket": "NODE_ENV=production node server.js",
    "start:next": "ts-node scripts/pre-startup.ts && next start",
    "// Other utilities": "",
    "lint": "next lint",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "postinstall": "prisma generate",
    "db:backup": "ts-node scripts/backup-database.ts",
    "db:restore": "ts-node scripts/restore-database.ts",
    "db:verify": "ts-node scripts/verify-database.ts",
    "db:check": "ts-node scripts/check-db.ts",
    "db:migrate": "ts-node scripts/safe-migrate.ts",
    "db:cron-backup": "ts-node scripts/cron-backup.ts",
    "db:add-security-events": "prisma migrate resolve --applied add_security_events && prisma db push",
    "db:reset-connection": "node scripts/reset-prisma-connection.js",
    "test:auth": "node scripts/test-auth-flows.js",
    "test:auth:quick": "node scripts/quick-auth-test.js",
    "test:auth:browser": "node scripts/browser-auth-test.js",
    "test:web-search": "node scripts/test-web-search.js",
    "vercel-build": "node scripts/fix-database-connection.js && node scripts/fix-next-auth-imports.js && tsc --noEmit && next lint && prisma generate && prisma db push --accept-data-loss && next build",
    "deploy": "node scripts/fix-database-connection.js && next build && vercel deploy --prod",
    "deploy:socket": "vercel deploy --prod",
    "fix-db": "node scripts/fix-database-connection.js"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.779.0",
    "@aws-sdk/s3-request-presigner": "^3.779.0",
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@giphy/js-fetch-api": "^5.6.0",
    "@giphy/react-components": "^10.0.1",
    "@hookform/resolvers": "^4.1.3",
    "@mux/mux-node": "^11.0.1",
    "@mux/mux-player-react": "^3.3.3",
    "@next-safe/middleware": "^0.10.0",
    "@prisma/client": "^6.5.0",
    "@sentry/browser": "^9.12.0",
    "@sentry/nextjs": "^9.12.0",
    "@stripe/stripe-js": "^7.0.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@upstash/redis": "^1.34.7",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.0.0",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "csrf": "^3.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "emoji-mart": "^5.6.0",
    "express-rate-limit": "^7.5.0",
    "framer-motion": "^12.6.2",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.484.0",
    "nanoid": "^5.1.5",
    "next": "^15.2.4",
    "next-auth": "^4.24.6",
    "next-safe": "^3.5.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.55.0",
    "react-hot-toast": "^2.5.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.2",
    "stripe": "^18.0.0",
    "styled-components": "^6.1.16",
    "swr": "^2.3.3",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie": "^0.6.0",
    "@types/csrf": "^1.3.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.13",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "9.24.0",
    "eslint-config-next": "15.2.4",
    "postcss": "^8.4.35",
    "prisma": "^6.5.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3"
  }
}
